<% if user_signed_in? %>
    <%= current_user.first_name %> 
    <%= current_user.last_name %>
    <%= current_user.state %>
    <%= current_user.address %>
    <%= current_user.phone %>

    <% @favorite_farmers.each do |farmer| %>
        <p><%= farmer.farmer.farm_name %>

    <% end %>
    
<% else %>
    <p>You need to sign in or sign up before continuing.</p>
<% end %>